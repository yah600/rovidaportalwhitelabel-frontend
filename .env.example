# Mock Data Configuration
# Set to "false" to disable mocks and use real API calls
VITE_USE_MOCKS=true

# Mock API delay in milliseconds (simulates network latency)
VITE_MOCK_DELAY=300

# Enable detailed mock logging in console
VITE_MOCK_DEBUG=false

# API Base URL (for when mocks are disabled)
# VITE_API_BASE_URL=https://api.example.com
